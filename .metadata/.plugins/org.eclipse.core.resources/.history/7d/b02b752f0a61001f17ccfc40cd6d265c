package com.capstone.backend.notifications;


import org.springframework.data.repository.CrudRepository;

public interface NotificationsRepository extends CrudRepository<Notifications, Long>{

	Object getReceiveEmailAlerts();

	Object getJobMatchAlerts();

	Object getApplicationStatusUpdates();

	Object getNewsletterSubscription();

	Object getEnablePushNotifications();

	Object getEndTime();

	Object getStartTime();

	Object getDoNotDisturb();
}